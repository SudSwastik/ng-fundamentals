{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            [
              "app",
              "events",
              "event",
              "events-app",
              "nav"
            ],
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            [
              "app",
              "nav",
              "events",
              "event",
              "events-app"
            ],
            "kebab-case"
        ]
    }
}
